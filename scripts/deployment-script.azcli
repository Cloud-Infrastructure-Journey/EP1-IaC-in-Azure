# Login to Azure
az login --use-device-code
az account show --query name

# List all RG names
az group list -o table

# create resource group
az group create --name ep-1-bicep-rg --location eastus2

# create storage account
az deployment group create --resource-group ep-1-bicep-rg --template-file .\deployment\main.bicep --parameters .\deployment\param.json -c
